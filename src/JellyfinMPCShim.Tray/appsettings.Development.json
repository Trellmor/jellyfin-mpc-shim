    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%LOCALAPPDATA%\\JellyfinMPCShim.Tray\\logs\\JellyfinMPCShim.Tray_.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      },
      {
        "Name": "Debug"
      }
    ]
  }
}
